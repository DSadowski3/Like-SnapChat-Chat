  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MuttyChat
	pod ‘Firebase’
	pod 'Firebase/Storage'
	pod ‘Firebase/Auth’

  target 'MuttyChatTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MuttyChatUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
